@model List<Cart>

    <table class="table table-hover">
        <tr>

       
        <th>
            اسم المنتج
        </th>
        <th>
           الكميه
        </th>
        <th>
            السعر
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>  <a href="/home/CurrentProduct/@item.ProductId">@item.Product.Name</a> </td>
            <td>  @item.Qty</td>
            <td>  @item.Product.Price</td>
           
        </tr>
    }
    
    </table>
    <a class="btn btn-success" href="/home/AddOrder">
        اكمال الطلب
        <i class="bi bi-plus"></i>
    </a>
